int mmagic_cli_ip_get_ip_addr(struct mmagic_data *core, EmbeddedCli *cli)
{
    struct mmagic_ip_data *data = mmagic_data_get_ip(core);

    char buf[MMAGIC_CLI_PRINT_BUF_LEN] = {0};
    /* -1 to allow for a NULL terminator at the end */
    int len = MMAGIC_CLI_PRINT_BUF_LEN - 1;
    int written = 0;

    written = snprintf(&buf[written], len, "%-30s", "ip.ip_addr");
    if (written < 0 || written > len)
    {
        return -1;
    }
    len -= written;
    written = mmagic_struct_ip_addr_to_string(&data->config.ip_addr, &buf[written], len);
    if (written < 0 || written > len)
    {
        return -1;
    }
    len -= written;

    embeddedCliPrint(cli, buf);

    return 0;
}

