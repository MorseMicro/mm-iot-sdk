#pragma once

/**
 * Process the received M2M buffer.
 *
 * @param ctx           Reference to the global mmagic data structure.
 * @param sid     The stream ID this buffer was received on.
 * @param header        The M2M header containing the subsystem, command and subcommand.
 * @param cmd_buf       The @c mmbuf with the buffer to be processed.
 * @return              An @c mmbuf with the response.
 */
static struct mmbuf* mmagic_m2m_process(struct mmagic_m2m_agent *agent,
    uint8_t sid, struct mmagic_m2m_command_header* header, struct mmbuf *cmd_buf)
{
    switch (header->subsystem)
    {
    case mmagic_wlan:
        return mmagic_m2m_wlan_process(agent, sid, header, cmd_buf);

    case mmagic_ip:
        return mmagic_m2m_ip_process(agent, sid, header, cmd_buf);

    case mmagic_ping:
        return mmagic_m2m_ping_process(agent, sid, header, cmd_buf);

    case mmagic_iperf:
        return mmagic_m2m_iperf_process(agent, sid, header, cmd_buf);

    case mmagic_sys:
        return mmagic_m2m_sys_process(agent, sid, header, cmd_buf);

    case mmagic_tcp:
        return mmagic_m2m_tcp_process(agent, sid, header, cmd_buf);

    default:
        return mmagic_m2m_create_response(header->subsystem, header->command,
            header->subcommand, MMAGIC_STATUS_NOT_FOUND, NULL, 0);
    }
}
